@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockGridItem>;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;

@{
    var quotationContent = Model?.Content.Value<string>("quotationContent");
        var blockquote = Model?.Content.Value<string>("blockquote");
}

@if(!string.IsNullOrEmpty(quotationContent)) 
{
    <q>@quotationContent</q>
}

@if(!string.IsNullOrEmpty("blockquote")) {
    <blockquote>@blockquote</blockquote>
}